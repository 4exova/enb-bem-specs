  "engines": {
    "node": ">= 0.10.0"
  },
  "peerDependencies": {
    "enb-magic-factory": "0.1.x"
  },
  "dependencies": {
    "bem-naming": "0.2.1",
    "vow": "0.4.5",
    "inherit": "2.2.2",
    "lodash": "2.4.1",
    "enb-bem": "0.1.0-alpha2",
    "enb-bem-pseudo-levels": "0.2.1",
